package com.meli.charge.api.response;

import java.util.Date;

public class TotalDebtResponse {

	private Integer userId;
	private Double totalDebt;
	private Date lastCharge;

	public TotalDebtResponse(Integer userId, Double totalDebt, Date lastCharge) {
		super();
		this.userId = userId;
		this.totalDebt = totalDebt;
		this.lastCharge = lastCharge;
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public Double getTotalDebt() {
		return totalDebt;
	}

	public void setTotalDebt(Double totalDebt) {
		this.totalDebt = totalDebt;
	}

	public Date getLastCharge() {
		return lastCharge;
	}

	public void setLastCharge(Date lastCharge) {
		this.lastCharge = lastCharge;
	}


}
