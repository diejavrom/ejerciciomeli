package com.meli.currency.api.response;

public class CurrencyConversionResponse {

	private String to;
	private Double amountTo;
	private Double amountFrom;
	
	public CurrencyConversionResponse() {
	}
	
	public CurrencyConversionResponse(String to, Double amountTo, Double amountFrom) {
		super();
		this.to = to;
		this.amountTo = amountTo;
		this.amountFrom = amountFrom;
	}

	public String getTo() {
		return to;
	}

	public void setTo(String to) {
		this.to = to;
	}

	public Double getAmounTo() {
		return amountTo;
	}

	public void setAmounTo(Double amounTo) {
		this.amountTo = amounTo;
	}

	public Double getAmounFrom() {
		return amountFrom;
	}

	public void setAmounFrom(Double amounFrom) {
		this.amountFrom = amounFrom;
	}

}
