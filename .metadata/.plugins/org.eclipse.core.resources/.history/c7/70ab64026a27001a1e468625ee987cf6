package com.meli.payment.model.to;

import java.io.Serializable;

public class TotalDebtTO implements Serializable {

	private static final long serialVersionUID = -7606412876851538738L;

	private Integer userId;
	private Double totalCharge;

	public TotalDebtTO() {
	}
	
	public TotalDebtTO(Integer userId, Double totalDebt) {
		super();
		this.userId = userId;
		this.totalCharge = totalDebt;
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public Double getTotalDebt() {
		return totalCharge;
	}

	public void setTotalDebt(Double totalDebt) {
		this.totalCharge = totalDebt;
	}

}
