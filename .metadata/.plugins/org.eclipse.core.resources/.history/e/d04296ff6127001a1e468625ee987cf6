package com.meli.charge.api.response;

import java.util.Date;

public class TotalChargeInfoResponse {

	private Integer userId;
	private Double totalCharge;
	private Date lastCharge;

	public TotalChargeInfoResponse(Integer userId, Double totalCharge, Date lastCharge) {
		super();
		this.userId = userId;
		this.totalCharge = totalCharge;
		this.lastCharge = lastCharge;
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public Double getTotalDebt() {
		return totalCharge;
	}

	public void setTotalDebt(Double totalDebt) {
		this.totalCharge = totalDebt;
	}

	public Date getLastCharge() {
		return lastCharge;
	}

	public void setLastCharge(Date lastCharge) {
		this.lastCharge = lastCharge;
	}

}