package com.meli.currency.api.response;

public class CurrencyConversionResponse {

	private String to;
	private Double amounTo;
	private Double amounFrom;
	
	public CurrencyConversionResponse() {
	}
	
	public CurrencyConversionResponse(String to, Double amountTo, Double amounFrom) {
		super();
		this.to = to;
		this.amounTo = amountTo;
		this.amounFrom = amounFrom;
	}

	public String getTo() {
		return to;
	}

	public void setTo(String to) {
		this.to = to;
	}

	public Double getAmounTo() {
		return amounTo;
	}

	public void setAmounTo(Double amounTo) {
		this.amounTo = amounTo;
	}

	public String getFrom() {
		return from;
	}

	public void setFrom(String from) {
		this.from = from;
	}

	public Double getAmounFrom() {
		return amounFrom;
	}

	public void setAmounFrom(Double amounFrom) {
		this.amounFrom = amounFrom;
	}

}
